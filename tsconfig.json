{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5",
    "lib": [
        "dom"
    ],
    "strict": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "downlevelIteration": true
  },
  "include": [
    "src/**.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
